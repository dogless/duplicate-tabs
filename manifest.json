{
  "name": "Duplicate",
  "version": "0.3",
  "description": "Creates a keyboard shortcut (Alt + Command + D) to toggle the pinned state of the currently selected tab",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "commands": {
    "MAC": {
      "suggested_key": { "default": "MacCtrl+D" },
      "description": "Duplicate"
    },
    "WINDOWS": {
      "suggested_key": { "default": ""},
      "description": "Duplicate"
    }
  },
  "manifest_version": 2
}
